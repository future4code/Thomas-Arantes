entes'\\n    }\\n\\n  componentDidUpdate() {\\n    const salvaTarefas = {\\n      id: this.state.id,\\n      texto: this.state.texto,\\n      completa: this.state.completa\\n    }\\n\\n    localStorage.setItem(\\\"tarefas-salvas\\\", JSON.stringify(salvaTarefas)) \\n\\n  };\\n\\n  componentDidMount() {\\n    const tarefasString = localStorage.getItem(\\\"tarefas-salvas\\\")\\n    const tarefasObjeto = JSON.parse(tarefasString)\\n    this.setState({\\n      id: tarefasObjeto.id,\\n      texto: tarefasObjeto.texto,\\n      completa: tarefasObjeto.completa\\n    })\\n\\n  };\\n\\n  onChangeInput = (event) => {\\n    this.setState({inputValue : event.target.value});\\n    \\n  }\\n\\n  criaTarefa = () => {\\n    const newTask = {\\n      id: Date.now(),\\n      texto: this.state.inputValue,\\n      completa: false\\n    }\\n\\n    const newTaskList = [...this.state.tarefas, newTask]\\n\\n    this.setState({tarefas : newTaskList})\\n  }\\n\\n  selectTarefa = (id) => {\\n    const novaListaDeTarefas = this.state.tarefas.map((tarefa) => {\\n      if(id === tarefa.id){\\n        const novaTarefa = {\\n          ...tarefa,\\n          completa: !tarefa.completa\\n        }\\n        return novaTarefa\\n      }\\n      else{\\n        return tarefa\\n      }\\n    })\\n\\n    this.setState({tarefas: novaListaDeTarefas})\\n\\n  }\\n\\n  onChangeFilter = (event) => {\\n    this.setState({filtro : event.target.value});\\n  }\\n\\n  render() {\\n    const listaFiltrada = this.state.tarefas.filter(tarefa => {\\n      switch (this.state.filtro) {\\n        case 'pendentes':\\n          return !tarefa.completa\\n        case 'completas':\\n          return tarefa.completa\\n        default:\\n          return true\\n      }\\n    })\\n\\n    return (\\n      <div className=\\\"App\\\">\\n        <h1>Lista de tarefas</h1>\\n        <InputsContainer>\\n          <input value={this.state.inputValue} onChange={this.onChangeInput}/>\\n          <button onClick={this.criaTarefa}>Adicionar</button>\\n        </InputsContainer>\\n        <br/>\\n\\n        <InputsContainer>\\n          <label>Filtro</label>\\n          <select value={this.state.filter} onChange={this.onChangeFilter}>\\n            <option value=\\\"\\\">Nenhum</option>\\n            <option value=\\\"pendentes\\\">Pendentes</option>\\n            <option value=\\\"completas\\\">Completas</option>\\n          </select>\\n        </InputsContainer>\\n        <TarefaList>\\n          {listaFiltrada.map(tarefa => {\\n            return (\\n              <Tarefa\\n                completa={tarefa.completa}\\n                onClick={() => this.selectTarefa(tarefa.id)}\\n              >\\n                {tarefa.texto}\\n              </Tarefa>\\n            )\\n          })}\\n        </TarefaList>\\n      </div>\\n    )\\n  }\\n}\\n\\nexport default App\\n\",\"import React from \\\"react\\\";\\nimport ReactDOM from \\\"react-dom\\\";\\nimport App from './App'\\n\\nconst rootElement = document.getElementById(\\\"root\\\");\\nReactDOM.render(<App />, rootElement);\\n\"]}","code":"(window.webpackJsonp=window.webpackJsonp||[]).push([[0],{13:function(t,e,a){t.exports=a(25)},19:function(t,e,a){},25:function(t,e,a){\"use strict\";a.r(e);var n=a(0),o=a.n(n),s=a(7),l=a.n(s),r=a(10),i=a(1),c=a(2);a(19);function u(){const t=Object(i.a)([\"\\n  display: grid;\\n  grid-auto-flow: column;\\n  gap: 10px;\\n\"]);return u=function(){return t},t}function p(){const t=Object(i.a)([\"\\n  text-align: left;\\n  text-decoration: \",\";\\n\"]);return p=function(){return t},t}function m(){const t=Object(i.a)([\"\\n  padding: 0;\\n  width: 200px;\\n\"]);return m=function(){return t},t}const h=c.a.ul(m()),d=c.a.li(p(),({completa:t})=>t?\"line-through\":\"none\"),f=c.a.div(u());var g=class extends o.a.Component{constructor(...t){super(...t),this.state={tarefas:[{id:Date.now(),texto:\"ir ao supermercado\",completa:!1},{id:Date.now(),texto:\"ir a farmacia\",completa:!0}],inputValue:\"\",filtro:\"pendentes\"},this.onCh